{
    "env": {
        "es6": true,
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended", "airbnb", "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "react/self-closing-comp": [1, {
            "component": true,
            "html": true
          }],
        "no-unused-vars": "warn"
    }
}